                    <Image url="@../../../../icons/settingDark.png" />
                </ImageView>
            </graphic>
            <StackPane.margin>
                <Insets right="40.0" />
            </StackPane.margin>
         <tooltip>
            <Tooltip text="Settings">
         <font>
                  <Font size="14.0" />
               </font>
            </Tooltip>
</tooltip>
