    <TextField fx:id="searchPatientField" focusTraversable="false" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" promptText="search patient (patient id)" stylesheets="@../../../../stylesheet/style7.css" visible="false" StackPane.alignment="TOP_CENTER">
